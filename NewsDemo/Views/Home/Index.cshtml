@{
    ViewData["Title"] = "Home Page";
}
@model IEnumerable<NewsDemo.Domain.Model.Menu>

<h1>Menu</h1>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.MenuId</td>
                <td>@item.Name</td>
                <td>@item.AccountType</td>
            </tr>
        }
    </tbody>
</table>